<?xml version="1.0" encoding="UTF-8" ?>
<portscope rows="4" columns="1" carrier="udp">

    <plot
        gridx="0" gridy="0" hspan="1" vspan="1" size="51" bgcolor="#EEFFEE"
        title="[Right Camera] CPU Usage (%)"
        minval="0.0" maxval="1.0" >
        <graph
            remote="/vpfR/scope:o"  index="8"
            size="2"  color="Black" type="lines" />
    </plot>

    <plot
        gridx="0" gridy="1" hspan="1" vspan="1" size="51" bgcolor="#EEFFEE"
        title="Event Rate (kHz)"
        minval="0.0" maxval="2000" >
        <graph
            remote="/vpfR/scope:o"  index="3"
            size="2"  color="Orange" type="lines" />
    </plot>

    <plot
        gridx="0" gridy="2" hspan="1" vspan="1" size="51" bgcolor="#EEFFEE"
        title="Filter Rate (Hz)"
        minval="0.0" maxval="2000" >
        <graph
            remote="/vpfR/scope:o"  index="1"
            size="2"  color="Blue" type="lines" />
    </plot>

    <plot
        gridx="0" gridy="3" hspan="1" vspan="1" size="51" bgcolor="#EEFFEE"
        title="Latency (ms)"
        minval="0.0" maxval="200" >
        <graph
            remote="/vpfR/scope:o"  index="0"
            size="2"  color="Red" type="lines" />
    </plot>

</portscope>
