proc eventPoints { x y } {
	upvar $x tmpX
	upvar $y tmpY
	bottle clear
	puts "will now check"
	eventPort read bottle true
	set tmpX [ [bottle get 0] asInt]
	set tmpY [ [bottle get 1] asInt]
}

proc attCmd { x y } {
	#upvar $x tmpX
	#upvar $y tmpY
